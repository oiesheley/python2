valor_conta = int(input("Digite o valor da conta: "))
notas_de_50 = valor_conta // 50
valor_conta %= 50
notas_de_10 = valor_conta // 10
valor_conta %= 10
notas_de_1 = valor_conta
print(f"Notas de 50: {notas_de_50}")
print(f"Notas de 10: {notas_de_10}")
print(f"Notas de 1: {notas_de_1}")





